{
  "name": "vue-tree-list",
  "version": "2.0.0-beta1",
  "description": "A Vue 3 component for tree structure. Support adding treenode/leafnode, editing node's name and dragging.",
  "author": "ayou",
  "type": "module",
  "main": "dist/vue-tree-list.umd.js",
  "module": "dist/vue-tree-list.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/vue-tree-list.es.js",
      "require": "./dist/vue-tree-list.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.vue",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "happy-dom": "^15.0.0",
    "less": "^4.2.0",
    "typescript": "^5.3.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "vue": "^3.4.0",
    "vue-tsc": "^2.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/ParadeTo/vue-tree-list/issues"
  },
  "homepage": "https://github.com/ParadeTo/vue-tree-list#readme",
  "keywords": [
    "vue",
    "vue3",
    "tree",
    "typescript"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ParadeTo/vue-tree-list.git"
  }
}